{% extends 'base.html.twig' %}


{% block body %}
</br>
<h5>Liste des compétence du professeur <em>{{professeur.prenom}} {{professeur.nom}}</em></h5>
</br>

<table class="table table-hover">
  <tr class="table-dark">
      <td>Compétences : </td>

  {% for e in professeur.competences %}

  <tr>
    <td>{{e.libelle}}</td>
  </tr>
  {% else %}
    <tr>
    <td>Aucun professeur n'a été trouvé.</td>
    </tr>



  {% endfor %}
</table>

{% endblock %}
